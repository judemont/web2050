{
    "manifest_version": 2,
    "name": "Web2050",
    "version": "1.0",
    "description": "A preview of the Internet's dystopian future.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/content/ads.js",
                "scripts/content/popup.js",
                "scripts/content/aichat.js",
                "scripts/content/aibanner.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "images/ads/*.png",
        "images/icons/*.png"
    ]
}